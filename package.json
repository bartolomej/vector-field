{
  "name": "simple-frontend-starter",
  "version": "1.0.0",
  "description": "Front end starter template with Babel, Webpack, Jest configurations.",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --watch",
    "deploy": "npm run build && node deploy.js",
    "build": "webpack --config webpack.prod.js",
    "start": "npm run serve && npm run watch",
    "serve": "webpack-dev-server --open --config webpack.dev.js"
  },
  "keywords": [
    "template"
  ],
  "author": "Bartolomej Kozorog",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "gh-pages": "^2.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-glob-entry": "^2.1.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "chroma-js": "latest",
    "monaco-editor": "^0.19.3",
    "vect-js": "^1.0.7"
  }
